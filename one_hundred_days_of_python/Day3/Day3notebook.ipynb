    "# odd or even calculator\n",
    "\n",
    "from math import remainder as rm\n",
    "\n",
    "integer = int(input(\"Enter an integer: \"))\n",
    "\n",
    "if rm(integer, 2) == 0:\n",
    "    print(\"Even\")\n",
    "else:\n",
    "    print(\"Odd\")"
   ]
